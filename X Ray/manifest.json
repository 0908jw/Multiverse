{
    "manifest_version": 3,
    "name": "X Ray Interface",
    "version": "1.0",
    "description": "An extension that exposes the hidden structure of browsing",
    "permissions": ["storage", "tabs"],
    "action": {
    },
    "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_idle"
    }
    ]
    }